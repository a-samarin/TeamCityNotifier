<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:TeamCityNotifier.WinUI.Resources.Styles">

    <SolidColorBrush x:Key="SuccessLightGreenBrush" Color="{StaticResource SuccessLightGreenColor}" />
    <SolidColorBrush x:Key="SuccessDarkGreenBrush" Color="{StaticResource SuccessDarkGreenColor}" />
    <SolidColorBrush x:Key="FailedLightRedBrush" Color="{StaticResource FailedLightRedColor}" />
    <SolidColorBrush x:Key="FailedDarkRedBrush" Color="{StaticResource FailedDarkRedColor}" />
</ResourceDictionary>
